				$(window).resize(function () {
			    if ($(window).width() < 768) {
				    console.log('mobile-size');
			        if ($('.trimslist').hasClass('off')) {
			            var owlActive = owl.owlCarousel(owlOptions);
			            owl.removeClass('off');
			        }
			    } else {
			        if (!$('.trimslist').hasClass('off')) {
		        		if( typeof owl.data("owlCarousel") != 'undefined' )
		        		{
		        	    	owl.addClass('off').data("owlCarousel").destroy(); // <-- This is returning undefined, causing JS errors
		        		}
			            owl.find('.owl-stage-outer').children(':eq(0)').unwrap();
			        }
			    }
			  });

			  var synccolor1 = $("#modelcolor");
			  var synccolor2 = $("#colorswatch");

			  synccolor1.owlCarousel({
			    singleItem : true,
			    slideSpeed : 1000,
			    navigation: false,
			    pagination: false,
			    afterAction : synccolorPosition,
			    mouseDrag : false,
					touchDrag : false,
			    responsiveRefreshRate : 200,
					transitionStyle: "fade"
			  });
              			  synccolor2.owlCarousel({
			    items : 12,
			    itemsDesktop      : [1199,12],
			    itemsDesktopSmall : [979,12],
			    itemsTablet       : [768,12],
			    itemsMobile       : [479,12],
			    navigation: false,
			    pagination: false,
			    responsiveRefreshRate : 100,
			    afterInit : function(el){
			      el.find(".owl-item").eq(0).addClass("synced");
			    }
			  });
                			  $(".color-next").click(function(){
			    synccolor1.trigger('owl.next');
			  })
			  $(".color-prev").click(function(){
			    synccolor1.trigger('owl.prev');
			  })

			  function synccolorPosition(el){
			    var colorcurrent = this.currentItem;
			    $("#colorswatch")
			      .find(".owl-item")
			      .removeClass("synced")
			      .eq(colorcurrent)
			      .addClass("synced")
			    if($("#colorswatch").data("owlCarousel") !== undefined){
			      colorcenter(colorcurrent)
			    }
			  }

			  $("#colorswatch").on("click", ".owl-item", function(e){
			    e.preventDefault();
			    var colornumber = $(this).data("owlItem");
			    synccolor1.trigger("owl.goTo",colornumber);
			  });

			  function colorcenter(number){
			    var sync2visible = synccolor2.data("owlCarousel").owl.visibleItems;
			    var num = number;
			    var found = false;
			    for(var i in sync2visible){
			      if(num === sync2visible[i]){
			        var found = true;
			      }
			    }

			    if(found===false){
			      if(num>sync2visible[sync2visible.length-1]){
			        synccolor2.trigger("owl.goTo", num - sync2visible.length+2)
			      }else{
			        if(num - 1 === -1){
			          num = 0;
			        }
			        synccolor2.trigger("owl.goTo", num);
			      }
			    } else if(num === sync2visible[sync2visible.length-1]){
			      synccolor2.trigger("owl.goTo", sync2visible[1])
			    } else if(num === sync2visible[0]){
			      synccolor2.trigger("owl.goTo", num-1)
			    }

			  }

			});
		})(jQuery);
