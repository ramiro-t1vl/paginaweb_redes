			jQuery('.mobile-tab-content.open').removeClass('open');
			jQuery(this).parent().find('.mobile-tab-content').addClass('open');
			jQuery('body').css('overflow', 'hidden');						
		}

	});
	
	jQuery('.mobile-tab-close').on('click', function(){
		jQuery('.mobile-tab-content.open').removeClass('open');
		jQuery('body').css('overflow', 'visible');
	});

} );
